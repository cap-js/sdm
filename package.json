{
  "name": "@cap-js/sdm",
  "version": "1.0.0",
  "description": "CAP plugin for effortless integration of CAP applications with SAP Document Management Service.",
  "main": "cds-plugin.js",
  "license": "Apache-2.0",
  "files": [
    "lib",
    "srv",
    "index.cds"
  ],
  "keywords": [
    "sap",
    "cap",
    "dms",
    "sdm",
    "plugin"
  ],
  "dependencies": {
    "@cap-js/attachments": "^1.0.1",
    "@sap/cds": "^7",
    "@sap/cds-lsp": "^7.6.1",
    "@sap/xssec": "^3.6.1",
    "axios": "^1.7.0",
    "dotenv": "^16.3.1",
    "express": "^4",
    "node-cache": "^5.1.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@eslint/js": "^9.4.0",
    "@sap/cds-dk": "^7",
    "eslint": "^9.4.0",
    "eslint-plugin-jest": "^28.5.0",
    "globals": "^15.3.0",
    "jest": "^29.7.0"
  },
  "scripts": {
    "start": "cds-serve",
    "test": "jest --coverage --config jest.config.js",
    "integration-test": "jest --config jest-integration.config.js",
    "lint": "npx eslint --fix ."
  },
  "cds": {
    "requires": {
      "kinds": {
        "sdm": {
          "impl": "@cap-js/sdm/lib/sdm"
        }
      },
      "[development]": {
        "attachments": {
          "kind": "sdm"
        }
      },
      "[production]": {
        "attachments": {
          "kind": "sdm"
        }
      },
      "[hybrid]": {
        "attachments": {
          "kind": "sdm"
        }
      }
    }
  }
}
